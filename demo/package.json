{
  "name": "interminimal-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3007 & cd .. && npm run dev",
    "build": "next build",
    "export": "npm run build && next export -o ../docs/demo",
    "start": "next start",
    "lint": "next lint"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "quoteProps": "consistent",
    "jsxSingleQuote": false,
    "trailingComma": "none",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "requirePragma": false,
    "insertPragma": false,
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css",
    "endOfLine": "lf"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "next": "^12.1.5",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "eslint": "^8.14.0",
    "eslint-config-next": "^12.1.5",
    "typescript": "^4.6.4"
  }
}
